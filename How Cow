#include<bits/stdc++.h>
using namespace std;

int main(){
    int n, c=1;
    cin>>n;

    while(n--){
        int x1,x2,y1,y2;

        cin>>x1 >>y1 >>x2 >>y2;

        int cow;
        cin>>cow;

        cout<<"Case " <<c <<':' <<endl;

        while(cow--){
            int a, b;
            cin>>a >>b;

            if(a>=x1 && a<=x2){
                if(b>=y1 && b<=y2){
                    cout<<"Yes" <<endl;
                }
                else{
                    cout<<"No" <<endl;
                }
            }
            else{
                cout<<"No" <<endl;
            }
        }
        c++;
    }
}
